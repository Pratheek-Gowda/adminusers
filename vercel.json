{
  "buildCommand": "echo 'Building Admin Dashboard'",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://refer.pratheek.shop/api"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://refer.pratheek.shop/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
